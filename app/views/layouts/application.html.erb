<body>

<div class="container">
<%= render "layouts/navBar" %>
</div>

<div class="container">
	<% flash.each do |name, msg| %>
		<%= content_tag(:div, msg, class: "alert alert-success") %>
		<% end %>
<%= yield %>
</div>


<div class="container">
<%= render "layouts/pagination" %>
